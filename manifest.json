{
    "name": "URL Summary",
    "version": "1.0.1",
    "manifest_version": 2,
    "description": "A Brave extension that summarizes the current tab URL.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    
    "browser_action": {
      "default_popup": "popup.html"
      
    },
    "permissions": ["activeTab"]
  }
  